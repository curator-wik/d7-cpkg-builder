{
    "name": "mbaynton/d7-cpkg-builder",
    "description": "Monitors for new Drupal 7 releases and generates new Curator updage cpkges",
    "type": "project",
    "require": {
        "sensiolabs/consul-php-sdk": "^3.1",
        "perchten/rmrdir": "dev-master",
        "curator/makeup": "dev-master",
        "php": "^7.0"
    },
    "autoload": {
        "files": ["src/drupalorg_api.php", "src/state.php", "src/makeup_invoker.php"]
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Baynton",
            "email": "mike@mbaynton.com"
        }
    ],
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/curator-wik/makeup"
        },
        {
            "type": "vcs",
            "url": "https://github.com/mbaynton/php-rmrdir.git"
        }
    ]
}
